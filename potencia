#programa com função que recebe dois parâmetros inteiros e calcule a potencia

def receber():
    a = int(input("Base da potencia: "))
    b = int(input("Exponente da potencia: "))
    return a, b

def potencia(a, b):
    pot = 1
    for _ in range(b):
        pot *= a
    return pot

def imprimir(x):
    print("O resultado da potência é:", x)

'''main'''
a, b = receber()
imprimir(potencia(a, b))
